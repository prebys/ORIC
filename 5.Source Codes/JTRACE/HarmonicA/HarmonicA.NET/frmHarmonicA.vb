	Public Function Jatn(ByRef X As Object, ByRef Y As Object) As Object
		' This routine calculates arc tangent in degrees over all 4 quadrants
		'   returns X=0, Y=0 as theta=0
		'
		If (X = 0#) Then
			If (Y = 0#) Then
				'UPGRADE_WARNING: Couldn't resolve default property of object Jatn. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
				Jatn = 0#
			Else
				'UPGRADE_WARNING: Couldn't resolve default property of object Jatn. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
				Jatn = 90#
			End If
		Else
			'UPGRADE_WARNING: Couldn't resolve default property of object X. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
			'UPGRADE_WARNING: Couldn't resolve default property of object Y. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
			'UPGRADE_WARNING: Couldn't resolve default property of object Jatn. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
			Jatn = System.Math.Atan(Y / X) * 57.2957795
		End If
		'
		If (X < 0#) Then
			'UPGRADE_WARNING: Couldn't resolve default property of object Jatn. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
			Jatn = Jatn + 180#
		Else
			If (Y < 0#) Then
				If (X = 0#) Then
					'UPGRADE_WARNING: Couldn't resolve default property of object Jatn. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
					Jatn = Jatn + 180#
				Else
					'UPGRADE_WARNING: Couldn't resolve default property of object Jatn. Click for more: 'ms-help://MS.VSCC.v90/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
					Jatn = Jatn + 360#
				End If
			End If
		End If
	End Function
	
